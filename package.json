    "canvas": "^2.7.0",
    "chalk": "^4.1.1",
    "config": "^3.3.6",
    "ffbinaries": "^1.1.4",
    "fluent-ffmpeg": "^2.1.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "ora": "^5.4.0",
    "rimraf": "^3.0.2"
  }
}
