{
  "name": "video-to-frames",
  "version": "0.0.1",
  "description": "影片分割工具",
  "main": "index.js",
  "author": "PSheon",
  "scripts": {
    "clean": "rimraf output/stage*/*.jpg output/stage*/*.json",
    "dev": "NODE_ENV=development node index.js",
